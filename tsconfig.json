{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "src/**/*",
    "src/global.d.ts"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "types": [
      "astro/client"
    ],
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}